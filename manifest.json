    "version": "0.1",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [ {
        "js": [ "jquery-2.1.3.min.js", "background.js" ],
        "matches": [ "https://www.xperteleven.com/*", "http://www.xperteleven.com/*"]
    }]
}